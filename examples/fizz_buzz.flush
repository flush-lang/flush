def fizz(n) {
    if (0 == n % 15) {
        putStrLn("FizzBuzz")
    } else {
        if (0 == n % 5) {
            putStrLn("Buzz")
        }

        if (0 == n % 3) {
            putStrLn("Fizz")
        } else {
            printLn(n)
        }
    }
}

def main() {
    for (i in range(1, 10)) {
        fizz(i)
    }
}
